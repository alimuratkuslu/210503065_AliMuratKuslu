<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: RGB(230,230,230);" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.ViewKundeController">
    <children>
        <VBox prefHeight="400.0" prefWidth="80.0" style="-fx-background-color: RGB(7,16,80);" />
        <TableView fx:id="kundeView" layoutX="100.0" layoutY="100.0" opacity="0.9" prefHeight="260.0" prefWidth="800.0">
            <columns>
                <TableColumn fx:id="ids" prefWidth="75.0" text="ID" />
                <TableColumn fx:id="namen" prefWidth="75.0" text="Name" />
                <TableColumn fx:id="vornamen" prefWidth="75.0" text="Vorname" />
                <TableColumn fx:id="telefonnummern" prefWidth="75.0" text="Telefon" />
                <TableColumn fx:id="ausweisnummern" prefWidth="75.0" text="Ausweis" />
                <TableColumn fx:id="versicherungTyps" prefWidth="75.0" text="Versicherung" />
                <TableColumn fx:id="adressen" prefWidth="75.0" text="Adresse" />
                <TableColumn fx:id="emails" prefWidth="75.0" text="Email" />
                <TableColumn fx:id="benutzerkontos" prefWidth="75.0" text="Username" />
                <TableColumn fx:id="passwörter" prefWidth="125.0" text="Passwort" />
            </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
        </TableView>
      <Text layoutX="125.0" layoutY="82.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Registrierte Kunden">
         <font>
            <Font name="Century Gothic" size="40.0" />
         </font>
      </Text>
      <Button layoutX="450.0" layoutY="365.0" mnemonicParsing="false" onAction="#redDashboard" text="Zurück zum Dashboard" />
    </children>
</AnchorPane>